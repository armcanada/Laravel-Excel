{
  "name": "armcanada/excel",
  "description": "Supercharged Excel exports in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "Maatwebsite.nl",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": ">=5.5",
    "phpoffice/phpexcel": "dev-dev-1.9",
    "illuminate/cache": "^5.0|^6.0|^7.0",
    "illuminate/config": "^5.0|^6.0|^7.0",
    "illuminate/filesystem": "^5.0|^6.0|^7.0",
    "illuminate/support": "^5.0|^6.0|^7.0",
    "jeremeamia/superclosure": "^2.3",
    "nesbot/carbon": "~1.0|^2.0",
    "tijsverkoyen/css-to-inline-styles": "~2.0"
  },
  "require-dev": {
    "rector/rector": "^0.9.7"
  },
  "suggest": {
    "illuminate/http": "^5.0|^6.0",
    "illuminate/routing": "^5.0|^6.0",
    "illuminate/view": "^5.0|^6.0",
    "illuminate/queue": "^5.0|^6.0"
  },
  "autoload": {
    "classmap": [
      "src/Maatwebsite/Excel"
    ],
    "psr-0": {
      "Maatwebsite\\Excel\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
        "laravel": {
            "providers": [
                "Maatwebsite\\Excel\\ExcelServiceProvider"
            ],
            "aliases": {
                "Excel": "Maatwebsite\\Excel\\Facades\\Excel"
            }
        }
    },
    "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/armcanada/PHPExcel"
      }
  ]
}
